{
  "name": "x-cropper",
  "version": "1.0.0",
  "description": "An image cropper and uploader component for VueJS and Vuetify",
  "author": "Nikita Kolyada <nikita.nk16@yandex.ru> (https://foma-blog.ru)",
  "license": "ISC",

  "scripts": {
    "serve": "yarn kill-front; vue-cli-service serve --watch ",
    "build-demo": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build --target lib --name XCropper ./src/components/XCropper/XCropper.vue",
    "fix": "eslint src/**/*.{vue,ts} --fix",
    "kill-front": "kill -9 `lsof -t -i tcp:8080`"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrsky1001/x-cropper.git"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "^2.6.11",
    "vuetify": "^2.5.9"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.14",
    "node-sass": "^4.12.0",
    "prettier": "^2.2.1",
    "sass-loader": "^8.0.2"
  },
  "bugs": {
    "url": "https://github.com/mrsky1001/x-cropper/issues"
  },
  "homepage": "https://github.com/mrsky1001/x-cropper#readme",
  "private": true,
  "keywords": [
    "Vue",
    "Vue.js",
    "vuejs",
    "image",
    "crop",
    "cropper",
    "uploader",
    "image cropper",
    "crop image",
    "image uploader",
    "cropping",
    "circular cropping",
    "circle crop",
    "avatar cropping"
  ],
  "main": "./dist/XCropper.umd.min.js",
  "unpkg": "./dist/XCropper.umd.min.js",
  "files": [
    "dist/XCropper.common.js",
    "dist/XCropper.common.js.map",
    "dist/XCropper.css",
    "dist/XCropper.umd.js",
    "dist/XCropper.umd.js.map",
    "dist/XCropper.umd.min.js",
    "dist/XCropper.umd.min.js.map"
  ]
}
